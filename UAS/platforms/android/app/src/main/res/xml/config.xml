<?xml version='1.0' encoding='utf-8'?>
<widget id="com.darmaji.uas" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>UAS</name>
    <description>
        Aplikasi uas mata kuliah platform mobile.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        DARMAJI PERSONAL
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="hdpi" src="/res/icon/android/icon-72.hdpi.png" />
    <icon density="hdpi" src="/res/icon/android/icon-96.xhdpi.png" />
    <icon density="hdpi" src="/res/icon/android/icon-144.xxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
